{
  "name": "push-anode",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:deploy": "npx prisma migrate deploy",
    "start": "npx prisma generate && npx prisma migrate deploy && nest start",
    "prestart:dev": "npm run lint",
    "start:dev": "npx prisma generate && npx prisma migrate deploy && nest start",
    "start:debug": "npx prisma generate && npx prisma migrate deploy && nest start --debug --watch",
    "start:prod": "npx prisma generate && npx prisma migrate deploy && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@nestjs/testing": "^10.3.10",
    "@types/chai": "^4.3.17",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "chai": "^5.1.1",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1"
  },
  "dependencies": {
    "@klerick/nestjs-json-rpc": "^1.0.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@prisma/client": "^5.18.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.14.1"
  }
}
