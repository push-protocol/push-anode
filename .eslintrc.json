{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "unused-imports/no-unused-imports": "off",
    "prefer-const": "off",
    "simple-import-sort/imports": "off",
    "simple-import-sort/exports": "off",
    "no-var": "off"
  }
}
