generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Block {
  hash       String   @id @default(uuid())
  metaData   Json
  timestamp  DateTime @default(now())
}

model Tx {
  hash       String   @id @default(uuid())
  blockHash  String
  category   String
  data       Json
  timestamp  DateTime @default(now())
}
